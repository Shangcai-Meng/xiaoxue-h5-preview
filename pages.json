	"subpackages": [
		{
			"root": "pages/common",
			"pages": [
				{
					"path": "richtext",
					"style": {
						"navigationStyle": "custom"
					}
				},
				{
					"path": "list-example",
					"style":{
						"navigationStyle": "custom"
					}
				}
			]
		},
		{
			"root": "pages/userSub",
			"pages": [
				{
					"path": "profile",
					"style": {
						"navigationBarTitleText": "用户详情"
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "TM3",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"bounce": "none"
		}
	},
	"tabBar": {
		"borderStyle": "#eaeaea",
		"color": "#9b9b9b",
		"selectedColor": "#ff7f32",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/index/index",
				"text": "首页",
				"iconPath": "static/tabbar/sy2.png",
				"selectedIconPath": "static/tabbar/sy1.png"
