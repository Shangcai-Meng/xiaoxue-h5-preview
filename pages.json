			},
			{
				"pagePath": "pages/options/index",
				"text": "期权",
				"iconPath": "static/tabbar/qq2.png",
				"selectedIconPath": "static/tabbar/qq1.png"
			},
			{
				"pagePath": "pages/mine/index",
				"text": "我的",
				"iconPath": "static/tabbar/wd2.png",
